#include "Pods-Tests-Kiwi.xcconfig"
CONFIGURATION_BUILD_DIR = $PODS_FRAMEWORK_BUILD_PATH
FRAMEWORK_SEARCH_PATHS = "$PODS_FRAMEWORK_BUILD_PATH" ${PODS_TESTS_KIWI_FRAMEWORK_SEARCH_PATHS}
FRAMEWORK_SEARCH_PATHS[sdk=iphoneos8.0] = ${PODS_TESTS_KIWI_FRAMEWORK_SEARCH_PATHS}
FRAMEWORK_SEARCH_PATHS[sdk=iphonesimulator8.0] = ${PODS_TESTS_KIWI_FRAMEWORK_SEARCH_PATHS}
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/Kiwi" "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase" "${PODS_ROOT}/Headers/Public/JMActionSheetDescription" "${PODS_ROOT}/Headers/Public/Kiwi"
OTHER_LDFLAGS = ${PODS_TESTS_KIWI_OTHER_LDFLAGS} -ObjC
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Tests
PODS_ROOT = ${SRCROOT}
SKIP_INSTALL = YES